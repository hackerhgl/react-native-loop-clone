{
  "name": "rnloop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "setup-ios": "cd node_modules/react-native && scripts/ios-install-third-party.sh && cd third-party/glog-0.3.5/ && ../../scripts/ios-configure-glog.sh",
    "setup-pods": "cd ios && pod install",
    "ios": "npx react-native run-ios --simulator=\"iPhone 8\"",
    "android": "npx react-native run-android",
    "clean-native": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && rm -rf .git/index.lock package-lock.json yarn.lock node_modules/ ios/build android/build/ android/app/build && npm i && cd ios && pod cache clean -all && pod install && cd .. && npm start -- --reset-cache",
    "clean-android": "rm -rf android/app/build",
    "extract-intl": "babel-node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "eslint": "eslint ./app",
    "eslint:fix": "eslint ./app --fix",
    "lint:staged": "lint-staged",
    "prettify": "prettier --config .prettierrc --write \"**/*.js\"",
    "tmp-fix": "babel-node ./temp/fix.js"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.json": "prettier --write"
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "@babel/runtime": "7.7.5",
    "@react-native-firebase/analytics": "^6.3.4",
    "@react-native-firebase/app": "^6.3.4",
    "@react-native-firebase/crashlytics": "^6.3.4",
    "@react-native-firebase/perf": "^6.3.4",
    "color": "3.1.2",
    "immutable": "3.8.2",
    "intl": "1.2.4",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-intl": "3.9.1",
    "react-native": "0.61.5",
    "react-native-device-info": "5.3.1",
    "react-native-gesture-handler": "1.5.2",
    "react-native-global-props": "1.1.5",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-reanimated": "1.4.0",
    "react-native-screens": "^2.0.0-alpha.18",
    "react-native-share": "^2.0.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-view-shot": "^3.0.2",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.10.3",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-immutable": "4.0.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "rn-hgl": "0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.7.4",
    "@babel/preset-env": "7.0.0",
    "@react-native-community/eslint-config": "0.0.5",
    "acorn": "7.1.1",
    "async": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-generator": "6.26.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "6.26.0",
    "babel-preset-react-native": "4.0.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-native": "3.8.1",
    "eslint-plugin-redux-saga": "1.1.1",
    "jetifier": "1.6.5",
    "lint-staged": "9.5.0",
    "metro-react-native-babel-preset": "0.57.0",
    "node-plop": "0.23.0",
    "plop": "2.5.3",
    "pre-commit": "1.2.2",
    "prettier": "1.19.1",
    "prettier-eslint-cli": "5.0.0",
    "prop-types": "15.7.2",
    "shelljs": "0.8.3"
  },
  "resolutions": {
    "metro-config": "0.57.0"
  }
}
